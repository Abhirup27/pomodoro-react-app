{
  "name": "Tomato Timer",
  "description": "A pomodoro timer made in react",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["storage"]
}
